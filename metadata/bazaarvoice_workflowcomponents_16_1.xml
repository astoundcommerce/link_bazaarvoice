<?xml version="1.0" encoding="UTF-8"?>
<custom-objects xmlns="http://www.demandware.com/xml/impex/customobject/2006-10-31">
    <custom-object type-id="WorkflowComponentDefinition" object-id="Bazaarvoice-PurchaseFeed">
        <object-attribute attribute-id="action">BazaarvoiceData-ExportPurchaseData</object-attribute>
        <object-attribute attribute-id="description" xml:lang="x-default">Export an order feed for Bazaarvoice</object-attribute>
    </custom-object>

    <custom-object type-id="WorkflowComponentDefinition" object-id="Bazaarvoice-ProductFeed">
        <object-attribute attribute-id="action">BazaarvoiceData-ExportProductCatalog</object-attribute>
        <object-attribute attribute-id="description" xml:lang="x-default">Export a product feed for Bazaarvoice</object-attribute>
    </custom-object>

    <custom-object type-id="WorkflowComponentDefinition" object-id="Bazaarvoice-InlineRatingsImport">
        <object-attribute attribute-id="action">BazaarvoiceInlineRatingData-ImportRatingFeed</object-attribute>
        <object-attribute attribute-id="description" xml:lang="x-default">Import Inline Rating data from Bazaarvoice</object-attribute>
    </custom-object>
</custom-objects>
