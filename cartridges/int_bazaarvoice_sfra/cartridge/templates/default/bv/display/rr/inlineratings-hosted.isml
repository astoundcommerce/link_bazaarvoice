<isif condition="${pdict.product && pdict.product.rating && pdict.product.rating.enabled && pdict.product.rating.type.equals('hosted')}">
	<isif condition="${pdict.product.bvProdId}">	
		<div data-bv-show="inline_rating" data-bv-productId="${pdict.product.bvProdId}" data-bv-redirect-url="${pdict.urls.product}"></div>
	</isif>
</isif>