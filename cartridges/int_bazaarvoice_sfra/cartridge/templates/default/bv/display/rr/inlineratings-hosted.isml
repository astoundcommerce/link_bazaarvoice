<!--- TEMPLATENAME: inlineratings-hosted.isml --->
${pdict.product}
<isif condition="${product && product.rating && product.rating.enabled && product.rating.type.equals('hosted')}">
	<isscript>
	var assets = require('*/cartridge/scripts/assets.js');
    assets.addJs(product.bvscout);
	</isscript>
		
	<isif condition="${product.bvProdId}">	
		<div data-bv-show="inline_rating" data-bv-productId="${product.bvProdId}" data-bv-redirect-url="${pdict.urls.product}"></div>
	</isif>
</isif>