<!--- TEMPLATENAME: test.isml --->
<html>
<head>
	<title>test Display codes</title>
	<style>
	.dcdiv {
		border: 1px solid black;
		padding: 15px;
		margin: 10px;
	}
	
	h1.dcheader {
		margin: 40px 0 0 10px;
	}
	</style>	
</head>

<body>

<h1 class="dcheader">Get Display Code (current DW locale '${pdict.CurrentRequest.locale}'):</h1>
<div class="dcdiv">
	<ul>
		<li>
			<h3>RR Display Code:</h3>
			<p>${pdict.RRDisplayCode}</p>
			<hr/>
		</li>
		<li>
			<h3>AA Display Code:</h3>
			<p>${pdict.AADisplayCode}</p>
			<hr/>
		</li>
		<li>
			<h3>SY Display Code:</h3>
			<p>${pdict.SYDisplayCode}</p>
			<hr/>
		</li>
		<li>
			<h1>Log:</h1>
			<p><isprint value="${pdict.ScriptLog.replace('\n', '<br />', 'g')}" encoding="off"/></p>
		</li>
	</ul>
</div>

<h1 class="dcheader">Validate Display Codes:</h1>
<div class="dcdiv">
	<ul>
		<li>
			<h3>Code Count:</h3>
			<p>${pdict.CodeCount}</p>
			<hr />
		</li>
		<li>
			<h3>SingleCode:</h3>
			<p>${pdict.SingleCode}</p>
			<hr />
		</li>
		<li>
			<h3>Valid Codes:</h3>
			<isloop items="${pdict.ValidCodes}" var="code">
				<p>${code}</p>
			</isloop>
			<hr />
		</li>
		<li>
			<h3>Message:</h3>
			<p>${pdict.Message}</p>
			<hr />
		</li>
		<li>
			<h3>Log:</h3>
			<p><isprint value="${pdict.VDCScriptLog.replace('\n', '<br />', 'g')}" encoding="off"/></p>
			<hr />
		</li>
	</ul>
</div>

<h1 class="dcheader">Import Display Codes:</h1>
<div class="dcdiv">
	<ul>
		<li>
			<isloop items="${pdict.ImportLocales.keySet()}" var="key">
				<p>${key} ==> ${pdict.ImportLocales.get(key)}</p>
			</isloop>
		</li>
		<li>
			<h3>Log:</h3>
			<p><isprint value="${pdict.ImportScriptLog.replace('\n', '<br />', 'g')}" encoding="off"/></p>
		</li>
	</ul>
</div>

<h1 class="dcheader">Export Display Codes:</h1>
<div class="dcdiv">
	<ul>
		<li>
			<isloop items="${pdict.ExportLocales.keySet()}" var="key">
				<p>${key} ==> ${pdict.ExportLocales.get(key)}</p>
			</isloop>
		</li>
		<li>
			<h3>Log:</h3>
			<p><isprint value="${pdict.ExportScriptLog.replace('\n', '<br />', 'g')}" encoding="off"/></p>
		</li>
	</ul>
</div>

</body>

</html>